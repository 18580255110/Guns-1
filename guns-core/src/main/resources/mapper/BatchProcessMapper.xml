<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.BatchProcessMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.BatchProcess">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="service" property="service" />
        <result column="description" property="description" />
        <result column="import_count" property="importCount" />
        <result column="complete_count" property="completeCount" />
        <result column="status" property="status" />
        <result column="work_status" property="workStatus" />
        <result column="import_date" property="importDate" />
        <result column="complete_date" property="completeDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, service, description, import_count, complete_count, status, work_status, import_date, complete_date
    </sql>

</mapper>
